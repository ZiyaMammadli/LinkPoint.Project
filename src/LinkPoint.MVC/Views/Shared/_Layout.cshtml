        <script>
        $(document).ready(function () {
            function positionDropdown() {
                var searchBox = $('#searchBox');
                var searchBoxOffset = searchBox.offset();
                var searchBoxHeight = searchBox.outerHeight();

                // Get the scroll position of the window
                var scrollTop = $(window).scrollTop();

                $('#searchResults').css({
                    top: searchBoxOffset.top + searchBoxHeight - scrollTop + "px", // Adjust position based on scroll
                    left: '50%',
                    transform: 'translateX(-50%) translateY(0)',
                    width: '500px'
                });
            }

            $('#searchBox').on('input', function () {
                var query = $(this).val();

                if (query.length >= 2) {
                    $.get('@Url.Action("SearchResults", "NewsFeed")', { query: query }, function (data) {
                        $('#searchResults').html(data).css({
                            display: "block",
                            opacity: 1,
                            visibility: 'visible'
                        }).addClass('show');
                        positionDropdown();
                    });
                } else {
                    $('#searchResults').removeClass('show').css({
                        display: 'none',
                        opacity: 0,
                        visibility: 'hidden'
                    });
                }
            });


            $(window).on('scroll resize', function () {
                if ($('#searchResults').hasClass('show')) {
                    positionDropdown();
                }
            });

            $(document).click(function (e) {
                if (!$(e.target).closest('#searchBox').length) {
                    $('#searchResults').removeClass('show').css({
                        display: 'none',
                        opacity: 0,
                        visibility: 'hidden'
                    });
                }
            });
        });
        </script>
       
